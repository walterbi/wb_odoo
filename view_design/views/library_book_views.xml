<odoo>
    <data>
        <record id="library_book_view_form" model="ir.ui.view">
            <field name="name">Create book</field>
            <field name="model">library.book</field>
            <field name="arch" type="xml">
                <form string="Create">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="qty"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>